<h1>Welcome!</h1>
<!-- <%= @places.inspect %> lists out all places-->
<hr />
<% @places.each do |place| %>
  <div class="booyah-box col-10 offset-1">
    <h1><%= place.name %></h1>
    <i><%= place.address %></i><br /><br />
    <p><%= place.description %></p><br />
  </div>
<% end %>

<div class="pagination">
  <%= paginate @places %>
</div>
